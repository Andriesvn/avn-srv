{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start": "nodemon -w *.*.ts --exec ts-node ./src/main.ts",
    "build": "npm run clean:dist && tsc && npm run start:prod",
    "clean:dist": "rd /s /q dist",
    "start:prod": "cd dist && node main.js"
  },
  "dependencies": {
    "@nestjs/common": "^4.5.9",
    "@nestjs/core": "^4.5.10",
    "@nestjs/cqrs": "^4.0.1",
    "@nestjs/microservices": "^4.5.8",
    "@nestjs/testing": "^4.5.5",
    "@nestjs/typeorm": "^2.0.0",
    "@nestjs/websockets": "^4.5.8",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.5",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "sqlite": "^2.9.1",
    "sqlite3": "^3.1.13",
    "typeorm": "^0.1.12",
    "typescript": "^2.6.2",
    "urlencode": "^1.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/lodash": "^4.14.104",
    "@types/node": "^9.3.0",
    "nodemon": "^1.17.1",
    "ts-node": "^4.1.0"
  }
}
